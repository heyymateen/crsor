{
  "name": "collaborative-code-editor",
  "version": "1.0.0",
  "description": "Real-time collaborative code editor with multi-user support, syntax highlighting, chat, and AI assistance",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "cors": "^2.8.5",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "collaborative",
    "code-editor",
    "real-time",
    "socket.io",
    "react",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}